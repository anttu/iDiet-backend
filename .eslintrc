{
    "parser": "babel-eslint",
    "plugins": ["class-property"],
    "extends": "airbnb",
    "ecmaFeatures": {
        "classes": true
    },
    "env": {
        "browser": true
    },
   "rules": {
        "properties": 0,
        "brace-style": [2, "1tbs"],
        "consistent-return": 2,
        "curly": [2, "multi-line"],
        "dot-notation": 2,
        "eol-last": 2,
        "indent": ["error", 4],
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-floating-decimal": 2,
        "no-param-reassign": 2,
        "no-self-compare": 2,
        "no-shadow": 0,
        "no-throw-literal": 2,
        "no-underscore-dangle": 0,
        "quotes": [2, "single"],
        "strict": 0,
        "vars-on-top": 2,
        "wrap-iife": 2,
        "semi": [2, "always"],
        "no-trailing-spaces": 2,
        "keyword-spacing": 2,
        "space-before-blocks": 2,
        "space-before-function-paren": [2, "never"],
        "space-infix-ops": 2,
        "spaced-comment": 2,
        "max-len": ["error", 140],
        // "jsx-indent": ["error", 4]
        "no-console": 0,
        "prefer-template": 0,
        prefer-spread: "error",
        "no-multiple-empty-lines": ["error", {"max": 1}],
        "global-require" : 1,
        "object-shorthand" : 0,
        "no-restricted-syntax": 0,
        "class-methods-use-this": 0,
    }
}
